# Chatwoot API base URL. Example: https://chatwoot.example.com/api/v1/accounts
CHATWOOT_URL=http://localhost:3000/api/v1/accounts

# Chatwoot account ID
CHATWOOT_ID=1

# Chatwoot inbox ID
CHATWOOT_INBOX_ID=1

# Chatwoot API access token
CHATWOOT_API_ACCESS_TOKEN=your_chatwoot_token

# Port for the application
PORT=3008

# Phone number that receives QR messages
PHONE_QR=+1234567890
